{
  "name": "zui",
  "version": "1.0.0",
  "description": "a simple and fast Vue component ",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "npm run dev:demo",
    "build:site": "npm run build:demo && npm run build:doc",
    "dev:demo": "cross-env NODE_ENV=development webpack-dev-server --hot -d --config webpack/webpack.demo.dev.config.js",
    "dev:doc": "cross-env NODE_ENV=development DOC_TYPE=true webpack-dev-server --hot -d -w --webpack build/webpack.doc.dev.config.js",
    "build:demo": "cross-env NODE_ENV=production webpack --hide-modules --progress --config webpack/webpack.demo.build.config.js",
    "build:doc": "cross-env NODE_ENV=production DOC_TYPE=true webpack --hide-modules --progress --config webpack/webpack.doc.build.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --hide-modules --progress --config webpack/webpack.prod.config.js",
    "build:prodmin": "cross-env NODE_ENV=production webpack --hide-modules --progress --config webpack/webpack.prod.mini.config.js"
  },
  "author": "huazite",
  "license": "ISC",
  "devDependencies": {
    "chokidar": "^3.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "eslint-loader": "^2.2.1",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "json-templater": "^1.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss": "^7.0.17",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^2.0.1",
    "vue-loader": "^15.7.0",
    "vue-markdown-loader": "^2.4.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.5.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "babel-loader": "^8.0.0-beta.0",
    "vue": "^2.6.10"
  }
}
